#include <iostream>
using namespace std;
int main() 
{
    int ele[5];
    cout<<"Enter elements: "<<endl;
    for(int i=0;i<5;++i)
    {
        cin>>ele[i];
    }
    cout<<"You entered: "<<endl;
    int *p=&ele[0];
    for(int n=0;n<5;n++)
    {
        cout<<ele[n]<<endl;
        p++;
    }
    return 0;
}
